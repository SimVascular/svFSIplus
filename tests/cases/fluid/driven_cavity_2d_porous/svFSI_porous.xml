<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>

  <Continue_previous_simulation> false </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 2 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 1000 </Number_of_time_steps> 
  <Time_step_size> 0.05 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.50 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> 1 </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 1 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 1 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 

</GeneralSimulationParameters>

<Add_mesh name="fluid" > 
  <Mesh_file_path> mesh/fluid/square_mesh_top.vtu  </Mesh_file_path>
  <Add_face name="fluid_left">
      <Face_file_path> mesh/fluid/square_mesh_top_left.vtp </Face_file_path>
  </Add_face>
  <Add_face name="fluid_top">
      <Face_file_path> mesh/fluid/square_mesh_top_top.vtp </Face_file_path>
  </Add_face>
  <Add_face name="fluid_right">
      <Face_file_path> mesh/fluid/square_mesh_top_right.vtp </Face_file_path>
  </Add_face>
  <Add_face name="fluid_bottom">
      <Face_file_path> mesh/fluid/square_mesh_top_bottom.vtp </Face_file_path>
  </Add_face>
  <Domain> 0 </Domain>

</Add_mesh>

<Add_mesh name="porous" >
  <Mesh_file_path> mesh/porous/square_mesh_bottom.vtu  </Mesh_file_path>
  <Add_face name="porous_left">
      <Face_file_path> mesh/porous/square_mesh_bottom_left.vtp </Face_file_path>
  </Add_face>
  <Add_face name="porous_top">
      <Face_file_path> mesh/porous/square_mesh_bottom_top.vtp </Face_file_path>
  </Add_face>
  <Add_face name="porous_right">
      <Face_file_path> mesh/porous/square_mesh_bottom_right.vtp </Face_file_path>
  </Add_face>
  <Add_face name="porous_bottom">
      <Face_file_path> mesh/porous/square_mesh_bottom_bottom.vtp </Face_file_path>
  </Add_face>
  <Domain> 1 </Domain>
</Add_mesh>

<Add_equation type="fluid" > 
   <Coupled> true </Coupled>
   <Min_iterations> 1 </Min_iterations>  
   <Max_iterations> 7</Max_iterations> 
   <Tolerance> 1e-12 </Tolerance> 
   <Backflow_stabilization_coefficient> 0.0 </Backflow_stabilization_coefficient> 

   <Domain id="0" >
      <Equation> fluid </Equation> 
      <Density> 1.0 </Density> 
      <Viscosity model="Constant" >
         <Value> 1e-06 </Value>
      </Viscosity>
      <Inverse_darcy_permeability> 0.0 </Inverse_darcy_permeability>
   </Domain>

   <Domain id="1" >
      <Equation> fluid </Equation> 
      <Density> 1.0 </Density> 
      <Viscosity model="Constant" >
         <Value> 1.111111111111111e-06 </Value>
      </Viscosity>
      <Inverse_darcy_permeability> 1e11 </Inverse_darcy_permeability> 
   </Domain>

   <Output type="Spatial" >
      <Velocity> true </Velocity>
      <Pressure> true </Pressure>
      <Traction> true </Traction>
      <Vorticity> true</Vorticity>
      <Divergence> true</Divergence>
      <WSS> true </WSS>
   </Output>

   <Output type="Volume_integral" >
     <Divergence> true </Divergence>
   </Output>

   <LS type="GMRES" >
      <Linear_algebra type="fsils" >
         <Preconditioner> fsils </Preconditioner>
      </Linear_algebra>
      <Max_iterations> 1000 </Max_iterations> 
      <Tolerance> 1e-12 </Tolerance>
   </LS>

   <Add_BC name="porous_left" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="porous_right" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="porous_bottom" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="porous_top" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="fluid_left" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="fluid_right" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="fluid_bottom" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value> 
   </Add_BC> 

   <Add_BC name="fluid_top" > 
      <Type> Dir </Type> 
      <Value> 0.1 </Value>
      <Effective_direction> (1, 0)</Effective_direction>
      <Zero_out_perimeter> false </Zero_out_perimeter>
   </Add_BC> 

   <Add_BC name="fluid_top" > 
      <Type> Dir </Type> 
      <Value> 0.0 </Value>
      <Effective_direction> (0, 1)</Effective_direction>
      <Zero_out_perimeter> false </Zero_out_perimeter>
   </Add_BC> 

</Add_equation>

</svFSIFile>